{
  "version": 2,
  "installCommand": "cd client && npm install",
  "buildCommand": "cd client && npx vite build",
  "outputDirectory": "client/dist",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}